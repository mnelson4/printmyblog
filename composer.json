{
    "name": "mnelson4/printmyblog",
    "description": "WordPress Plugin that simplifies printing your entire blog.",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "szepeviktor/phpstan-wordpress": "^0.5.0",
        "squizlabs/php_codesniffer": "^3.5.4"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "mnelson4\\": "src/mnelson4/",
            "PrintMyBlog\\": "src/PrintMyBlog/",
            "Twine\\": "src/Twine/"
        }
    },
    "scripts": {
        "release": "git archive HEAD --format=zip --output ../printmyblog.zip"
    }
}
